{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ],
    "functions": {
      "api/*.js": {
        "maxDuration": 10
      }
    },
    "headers": [
      {
        "source": "/api/webhook-inbound",
        "headers": [
          {
            "key": "X-Vercel-Protection-Bypass",
            "value": "$VERCEL_PROTECTION_BYPASS"
          }
        ]
      }
    ]
  }